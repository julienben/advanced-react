    "babel-plugin-transform-react-remove-prop-types": "0.4.23",
    "cheerio": "1.0.0-rc.2",
    "circular-dependency-plugin": "5.0.2",
    "css-loader": "2.1.0",
    "eslint": "5.12.1",
